
def merge_and_sort(list1, list2):

    combined_list = list1 + list2
    print("merged list:")
    print(combined_list)
    
    combined_list.sort()
    
    return combined_list

list1 = [3, 1, 4]
list2 = [2, 5, 6]
sorted_list = merge_and_sort(list1, list2)
print("merged and sorted list:")
print(sorted_list)
